{
  "name": "@devimasolutions/auth",
  "version": "1.0.0",
  "description": "Auth boilerplate for frontend JS project",
  "main": "lib/index",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "dev": "cd example && yarn dev",
    "lint": "eslint src",
    "typescript": "tsc --noEmit",
    "setup": "rimraf .husky/pre-commit .husky/pre-push .husky/commit-msg && husky add .husky/pre-commit \"npx lint-staged\" && husky add .husky/pre-push \"yarn prepush\" && husky add .husky/commit-msg \"npx --no -- commitlint --edit $1\" && husky install",
    "prepush": "tsc --noEmit && jest",
    "build": "tsc -p tsconfig-build.json",
    "prepare": "yarn build",
    "publish": "npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DevimaSolutions/auth.git"
  },
  "keywords": [
    "auth",
    "jwt",
    "boilerplate",
    "typescript",
    "react"
  ],
  "author": "DevimaSolutions <welcome@devima.solutions> (https://github.com/DevimaSolutions)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DevimaSolutions/auth/issues"
  },
  "homepage": "https://github.com/DevimaSolutions/auth#readme",
  "dependencies": {},
  "peerDependencies": {
    "axios": ">= 0.17.0 < 1.0.0",
    "react": ">= 16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/config-conventional": "^16.0.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.20.0",
    "babel-jest": "^29.4.2",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
